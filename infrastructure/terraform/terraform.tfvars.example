# Terraform variables configuration
# Copy this file to terraform.tfvars and modify the values as needed

resource_group_name = "rg-isapi-migration"
location           = "East US"
app_name          = "delphi-isapi-app"
app_service_plan_sku = "S1"
enable_application_insights = true
enable_azure_files = true
file_share_name = "isapi-shared-folder"

tags = {
  Environment = "Development"
  Project     = "ISAPI Migration"
  ManagedBy   = "Terraform"
  Owner       = "Development Team"
}
